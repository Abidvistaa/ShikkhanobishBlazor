
@page "/Index"

<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8" />
    <title>Dashboard</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta content="Responsive bootstrap 4 admin template" name="description" />
    <meta content="Coderthemes" name="author" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />

</head>

<body>

    <!-- Begin page -->
    <div id="wrapper">

        <!-- Topbar Start -->
        <div class="navbar-custom">
            <ul class="list-unstyled topnav-menu float-right mb-0">
                <li class="dropdown notification-list">
                    <a class="nav-link dropdown-toggle nav-user mr-0" data-toggle="dropdown" href="#" role="button" aria-haspopup="false" aria-expanded="false">
                        <img src="/images/images.png" alt="user-image" class="rounded-circle" />
                        <span class="pro-user-name ml-1"> Maxine K <i class="mdi mdi-chevron-down"></i> </span>
                    </a>
                    <div class="dropdown-menu dropdown-menu-right profile-dropdown">
                        <!-- item-->
                        <a href="javascript:void(0);" class="dropdown-item notify-item">
                            <i class="mdi mdi-logout-variant"></i>
                            <span>Logout</span>
                        </a>
                    </div>
                </li>
            </ul>

            <!-- LOGO -->
            <div class="logo-box">
                <a href="Index" class="logo text-center logo-dark">
                    <span class="logo-lg">
                        <img src="/images/logo1.png" alt="" height="50" />
                    </span>

                    <span class="logo-sm">
                        <img src="/images/logo1.png" alt="" height="22" />
                    </span>
                </a>

                <a href="Index" class="logo text-center logo-light">
                    <span class="logo-lg">
                        <img src="/images/logo1.png" alt="" height="50" />
                    </span>
                    <span class="logo-sm">
                        <img src="/images/logo1.png" alt="" height="22" />
                    </span>
                </a>
            </div>

            <ul class="list-unstyled topnav-menu topnav-menu-left m-0">
                <li>
                    <button class="button-menu-mobile">
                        <i class="mdi mdi-menu"></i>
                    </button>
                </li>
            </ul>
        </div>
        <!-- end Topbar -->
        <!-- Left Sidebar Start  -->
        <div class="left-side-menu">
            <div class="user-box">
                <div class="float-left">
                    <img src="/images/images.png" alt="" class="avatar-md rounded-circle" />
                </div>
                <div class="user-info">
                    <a href="#">Maxine K</a>
                </div>
            </div>

            <!--- Sidemenu -->
            <div id="sidebar-menu">
                <ul class="metismenu" id="side-menu">
                    <li class="menu-title">Navigation</li>

                    <li>
                        <a href="https://dashboard.shikkhanobish.com/index.html">
                            <i class="fas fa-home"></i>
                            <span> Dashboard </span>
                        </a>
                    </li>
                    <li>
                        <a href="/">
                            <i class="fas fa-calculator"></i>
                            <span> Count </span>
                        </a>
                    </li>

                    <li>
                        <a href="Questions">
                            <i class="far fa-question-circle"></i>
                            <span> Question </span>
                        </a>
                    </li>
                    <li>
                        <a href="ActiveTeacher">
                            <i class="fas fa-lightbulb"></i>
                            <span> Active Teacher </span>
                        </a>
                    </li>

                    <li>
                        <a href="TeacherApproval">
                            <i class="fas fa-user-alt"></i>
                            <span> Teacher Approval </span>
                        </a>
                    </li>
                    <li>
                        <a href="TeacherWithdraw">
                            <i class="fas fa-money-bill-wave-alt"></i>
                            <span> Teacher Withdraw </span>
                        </a>
                    </li>
                </ul>
            </div>
            <!-- End Sidebar -->

            <div class="clearfix"></div>
        </div>
        <!-- Left Sidebar End -->
        <!-- ============================================================== -->
        <!-- Start Page Content here -->
        <!-- ============================================================== -->


        <div class="content-page">
            <div class="content">

                <!-- Start container-fluid -->
                <div class="container-fluid">
                    

                    <div class="row">
                        <div class="col-lg-6">
                            <div class="mt-5">
                                <h5 class="font-14">Institution</h5>
                                <canvas id="institute-v-firstchoice-bar"></canvas>
                            </div>
                        </div>
                        <!--// morris-bar-chart -->

                        <div class="col-lg-6">
                            <div class="mt-5">
                                <h5 class="font-14">ClassInfo</h5>
                                <canvas id="classinfo-v-secondchoice-bar"></canvas>
                            </div>
                        </div>
                        <!--// morris-bar-chart2 -->

                        <div class="col-lg-6">
                            <div class="mt-5">
                                <h5 class="font-14">Subject</h5>
                                <canvas id="subject-v-thirdchoice-bar"></canvas>
                            </div>
                        </div>
                        <!--// morris-bar-chart3 -->

                        <div class="col-lg-6">
                            <div class="mt-5">
                                <h5 class="font-14">Chapter</h5>
                                <canvas id="chapter-v-forthchoice-bar"></canvas>
                            </div>
                        </div>
                        <!--// morris-bar-chart4 -->

                        <div class="col-lg-6">
                            <div class="mt-5">
                                <h5 class="font-14">Income Per Subject</h5>
                                <canvas id="subject-v-income-bar"></canvas>
                            </div>
                        </div>
                        <!--// morris-bar-chart6 -->



                        <div class="col-lg-6">
                            <div class="mt-5">
                                <h5 class="font-14">Income Per Chapter</h5>
                                <canvas id="chapter-v-income-bar"></canvas>
                            </div>
                        </div>
                        <!--// morris-bar-chart7 -->




                        <div class="col-lg-6">
                            <div class="mt-5">
                                <h5 class="font-14">Tuition Per Day</h5>
                                <canvas id="tuition-v-date-line"></canvas>
                            </div>
                        </div>
                        <!--// morris-line-chart -->
                    </div>
                    <br />
                    <!-- end row -->
                    <!-- Teacher Withdraw Reqest -->
                    <div class="row">
                        <div class="col-md-12">
                            <div class="card teacherWithdrawTable">
                                <div class="card-header">
                                    <h5>Withdraw Request</h5>
                                </div>
                                <div class="card-body">
                                    <div class="table-area">
                                        <!-- table-responsive -->
                                        <div class="table-responsive">
                                            <table id="all_withdraw_list" class="table table-bordered">
                                            </table>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <br />
                <!-- end container-fluid -->
                <!-- Footer Start -->
                <footer class="footer">
                    <div class="container-fluid">
                        <div class="row">
                            <div class="col-md-12">
                                2021 &copy; shikkhanobish
                            </div>
                        </div>
                    </div>
                </footer>
                <!-- end Footer -->
            </div>
            <!-- end content -->

        </div>
        <!-- END content-page -->

    </div>
    <!-- END wrapper -->
    <!-- Vendor js -->
</body>
</html>


